    "dotenv": "^16.3.1",
    "natural": "^6.7.0",
    "@google/generative-ai": "^0.11.0",
    "google-auth-library": "^9.6.0"
  },
  "devDependencies": {
    "shx": "^0.4.0",
    "ts-node": "^10",
